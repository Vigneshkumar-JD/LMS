    margin-left: 15px;
    border: solid 7px var(--color3);
    border-radius: 50%;

    /* Flex */
    display: flex;
    justify-content: center;
    align-items: center;
}

.container__subFeat--right .container__subFeatImgContent {
    margin-left: 0;
    margin-right: 15px;
}

.container__subFeatIcon {
    width: 40px;
    height: 40px;
}

.container__subFeatContent {
    max-width: 500px;
    padding: 15px;
